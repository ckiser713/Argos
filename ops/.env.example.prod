# Core
ARGOS_ENV=strix
ARGOS_AUTH_SECRET=  # Generate: openssl rand -hex 32
ARGOS_DOMAIN=your-domain.com
ARGOS_ADMIN_EMAIL=admin@your-domain.com

# Database
POSTGRES_USER=cortex
POSTGRES_PASSWORD=  # Generate strong password
REDIS_PASSWORD=  # Generate strong password

# Storage
MINIO_ROOT_USER=argos-admin
MINIO_ROOT_PASSWORD=  # Generate strong password
ARGOS_STORAGE_BACKEND=s3
ARGOS_STORAGE_BUCKET=argos-ingest

# n8n (optional)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=  # Generate strong password
N8N_ENCRYPTION_KEY=  # Generate: openssl rand -hex 32

# Lanes (customize per deployment)
ARGOS_LANE_SUPER_READER_URL=http://llama-super-reader:8080/v1
ARGOS_LANE_GOVERNANCE_URL=http://llama-governance:8081/v1
ARGOS_LANE_ORCHESTRATOR_URL=http://inference-vllm:8000/v1
ARGOS_LANE_CODER_URL=http://inference-vllm:8000/v1
ARGOS_LANE_FAST_RAG_URL=http://inference-vllm:8000/v1

# Startup
ARGOS_STRICT_LANE_STARTUP=true
ARGOS_LANE_WARMUP_TIMEOUT=30
